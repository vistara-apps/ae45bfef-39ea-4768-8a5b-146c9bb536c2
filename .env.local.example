# Database
DATABASE_URL="postgresql://username:password@localhost:5432/patrolproof"

# OnchainKit API Key (get from https://portal.cdp.coinbase.com/)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_api_key_here

# Pinata IPFS API Keys (get from https://pinata.cloud/)
PINATA_API_KEY=your_pinata_api_key
PINATA_SECRET_KEY=your_pinata_secret_key

# OpenAI API Key (for AI incident summaries)
OPENAI_API_KEY=your_openai_api_key

# Resend API Key (for client reports)
RESEND_API_KEY=your_resend_api_key

# Mapbox API Key (get from https://account.mapbox.com/)
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_token

# Base RPC URL
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# JWT Secret for API authentication
JWT_SECRET=your_jwt_secret_here

# Farcaster App Secret (for frame verification)
FARCASTER_APP_SECRET=your_farcaster_app_secret

# Company wallet private key (for gasless transactions)
COMPANY_WALLET_PRIVATE_KEY=your_company_wallet_private_key
